@model List<ResultAboutDto>

@{
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table tavle-bordered">
    <tr>
        <th>#</th>
        <th>Title 1</th>
        <th>Title 2</th>
        <th>Content</th>
        <th>Update</th>

    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.AboutID</td>
            <td>@item.Title1</td>
            <td>@item.Title2</td>
            <td>@item.Content</td>
            <td>
                <a href="/AdminAbout/UpdateAbout/@item.AboutID" class="btn mb-1 btn-rounded btn-outline-success">Update</a>
            </td>
        </tr>
    }
</table>